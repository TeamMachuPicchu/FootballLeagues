@model IEnumerable<FootballLeagues.Web.Models.League.LeagueViewModel>

@foreach (var item in Model)
{
    var imgSrc = string.Format("data:image/jpg;base64,{0}", item.Logo);
    
    <a href="@Url.Action("Details", "Leagues", new {id = item.Id})" class="btn btn-warning">
        <img src="@imgSrc" alt="Logo" width="200" />
    </a>
}
